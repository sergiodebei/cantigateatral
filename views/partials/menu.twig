{% if menu %}
    <ul class="menu">
        {% for item in menu %}
            <li class="menu-item">
                <a target="{{ item.target }}" href="{{ item.link }}">{{ item.title }}</a>
                {% include "partials/menu.twig" with {'menu': item.get_children} %}
            </li>
        {% endfor %}

	</ul>
